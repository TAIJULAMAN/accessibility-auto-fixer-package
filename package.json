{
    "name": "accessibility-auto-fixer",
    "version": "1.0.1",
    "description": "Automatically detect and fix accessibility issues in your HTML, JSX, and TSX code",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.js",
    "bin": {
        "a11y-fix": "dist/cli.js",
        "accessibility-auto-fixer": "dist/cli.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TAIJULAMAN/accessibility-auto-fixer-package.git"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "test": "jest",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "accessibility",
        "a11y",
        "auto-fix",
        "aria",
        "wcag",
        "html",
        "jsx",
        "tsx",
        "react",
        "react-hooks",
        "linting",
        "fixing"
    ],
    "author": "Md Shah Aman Patwary <mdshahamanpatwary@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@babel/parser": "^7.23.0",
        "@babel/traverse": "^7.23.0",
        "@babel/types": "^7.23.0",
        "chalk": "^4.1.2",
        "commander": "^11.1.0",
        "glob": "^10.3.10",
        "jsdom": "^23.0.0"
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "devDependencies": {
        "@types/babel__traverse": "^7.20.5",
        "@types/jest": "^29.5.8",
        "@types/jsdom": "^21.1.7",
        "@types/node": "^20.9.0",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "jest": "^29.7.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "ts-jest": "^29.1.1",
        "typescript": "^5.2.2"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./react": {
            "types": "./dist/react/index.d.ts",
            "default": "./dist/react/index.js"
        },
        "./cli": {
            "types": "./dist/cli.d.ts",
            "default": "./dist/cli.js"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "engines": {
        "node": ">=14.0.0"
    }
}
